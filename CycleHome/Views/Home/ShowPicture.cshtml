@model IEnumerable<CycleHome.Picture>

@{
    ViewBag.Title = "ShowPicture";
}

<h2>Cycle Pricture</h2>
<table>
    <tr>
        @foreach (var item in Model)
        {
            <td>
                @{
                    var base64 = Convert.ToBase64String(item.PictureTitle);
                    var picsrc = string.Format("data:image/gif;base64,{0}", base64);
                }
                <img src="@picsrc" style="max-width:300px;max-height:200px" />
            </td>
        }

    </tr>
</table>

